# SYMPHONI.A - Configuration Backend APIs PRODUCTION
# Copier ce fichier vers .env.production et remplir avec vos URLs de production

# API Principale
NEXT_PUBLIC_API_URL=https://api.symphonia.com/api/v1

# API Commandes
NEXT_PUBLIC_ORDERS_API_URL=https://api-orders.symphonia.com/api/v1

# API Tracking
NEXT_PUBLIC_TRACKING_API_URL=https://api-tracking.symphonia.com/api/v1

# API Rendez-vous
NEXT_PUBLIC_APPOINTMENTS_API_URL=https://api-appointments.symphonia.com/api/v1

# API Documents
NEXT_PUBLIC_DOCUMENTS_API_URL=https://api-documents.symphonia.com/api/v1

# API Scoring
NEXT_PUBLIC_SCORING_API_URL=https://api-scoring.symphonia.com/api/v1

# API Affret.IA
NEXT_PUBLIC_AFFRET_API_URL=https://api-affret.symphonia.com/api/v1

# WebSocket
NEXT_PUBLIC_WS_URL=wss://ws.symphonia.com

# AWS (pour documents)
NEXT_PUBLIC_AWS_REGION=eu-west-1
NEXT_PUBLIC_AWS_S3_BUCKET=symphonia-documents-prod

# TomTom (pour tracking premium)
NEXT_PUBLIC_TOMTOM_API_KEY=your_production_tomtom_api_key

# Google Maps (fallback)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_production_google_maps_api_key

# Environnement
NEXT_PUBLIC_ENV=production
